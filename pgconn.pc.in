prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: pgconn
Description: libpq-based PostgreSQL connection pool
Version: @PROJECT_VERSION@
Requires: libpq
Libs: -L${libdir} -lpgconn -lpthread
Cflags: -I${includedir}/pgconn

